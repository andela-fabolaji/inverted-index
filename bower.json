{
  "name": "inverted-index",
  "description": "A light-weight full-text search app that uses elastic search",
  "main": "app.js",
  "authors": [
    "Abolaji Femi"
  ],
  "license": "ISC",
  "keywords": [
    "text",
    "search"
  ],
  "homepage": "https://github.com/femi-abolaji/inverted-index",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "boostrap#^3.3.7",
    "font-awesome": "^4.7.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.min.css",
        "less/bootstrap.less"
        ]
    },
    "font-awesome": {
      "main": [
        "less/font-awesome.less",
        "css/font-awesome.min.css",
        "scss/font-awesome.scss"
      ]
    }
  }
}
